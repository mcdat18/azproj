{
	"name": "DS_ADLS2_Parquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_mcdedevdlake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"containerName": {
				"type": "string"
			},
			"folderName": {
				"type": "string"
			},
			"dbName": {
				"type": "string"
			},
			"schemaName": {
				"type": "string"
			},
			"tableName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "SQLDynamicLoad"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@{item().src_table}",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().folderName,'/', dataset().dbName, '/',dataset().schemaName, '/' ,'parquet', '/' ,formatDateTime(convertFromUtc(utcnow(),'Eastern Standard Time'),'yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().containerName",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}